{
  "name": "docs-generator",
  "version": "1.0.0",
  "description": "Generador de doucmentaciones t√©cnicas",
  "main": "index.js",
  "watch": {
    "test": {
      "patterns": [
        "src/**/*"
      ],
      "delay": 2500,
      "extensions": "js,md,mdx"
    }
  },
  "scripts": {
    "test": "npm-watch && npm run doc:gen",
    "doc:gen": "apidoc -i src/ -o docs/",
    "format": "prettier --write .",
    "lint": "eslint --fix . --ext .js"
  },
  "dependencies": {
    "npm-watch": "^0.13.0",
    "onchange": "^7.1.0"
  },
  "devDependencies": {
    "apidoc": "^1.2.0",
    "@eslint/js": "^9.4.0",
    "eslint": "^9.4.0",
    "globals": "^15.4.0",
    "prettier": "^3.3.2"
  },
  "contributors": [
    {
      "name": "Alexander Porro",
      "email": "alex1020pl@gmail.com",
      "url": "https://github.com/porrodv"
    }
  ],
  "author": "Alexander Porro <alex1020pl@gmail.com>",
  "license": "MIT"
}
